<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background"
        android:paddingTop="5dip"
        android:paddingBottom="5dip">
        <shape>
            <corners android:radius="2dip" />
            <gradient
                android:startColor="#ffdbdedd"
                android:endColor="#ffdbdedd"
                android:centerY="0"
                android:angle="0"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress"
        android:paddingTop="5dip"
        android:paddingBottom="5dip" >
        <clip>
            <shape>
                <corners android:radius="2dip" />
                <gradient
                    android:startColor="#ff009ce4"
                    android:endColor="#ff009ce4"
                    android:centerY="0"
                    android:angle="0"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress"
        android:paddingTop="5dip"
        android:paddingBottom="5dip" >
        <clip>
            <shape>
                <corners android:radius="2dip" />
                <gradient
                    android:startColor="#ff009ce4"
                    android:endColor="#ff009ce4"
                    android:centerY="0"
                    android:angle="0"/>
            </shape>
        </clip>
    </item>
</layer-list>